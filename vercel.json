{
    "version": 2,
    "builds": [
        {
            "src": "semantic_backend/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python"
            }
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "semantic_backend/wsgi.py"
        }
    ]
}